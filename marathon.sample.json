{
    "id": "cors-anywhere",
    "cmd": "node server.js",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "lippo/cors-anywhere",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "hostPort": 0,
                "containerPort": 8080
            }]
        }
    },
    "instances": 2,
    "cpus": 2,
    "mem": 4096,
    "healthChecks": [{
        "protocol": "MESOS_HTTP",
        "path": "/",
        "portIndex": 0,
        "timeoutSeconds": 60,
        "gracePeriodSeconds": 15,
        "intervalSeconds": 3,
        "maxConsecutiveFailures": 2
    }],
    "labels": {
        "HAPROXY_DEPLOYMENT_GROUP": "cors-anywhere",
        "HAPROXY_GROUP": "external",
        "HAPROXY_0_VHOST": "services.mataharimall.co",
        "HAPROXY_0_HTTP_BACKEND_PROXYPASS_PATH": "/cors-anywhere",
        "HAPROXY_0_PATH": "/cors-anywhere",
        "HAPROXY_0_REDIRECT_TO_HTTPS": "true"
    }
}